{
    "$schema": "https://getcomposer.org/schema.json",
    "name": "laravel/vue-starter-kit",
    "type": "project",
    "description": "The skeleton application for the Laravel framework.",
    "keywords": [
        "laravel",
        "framework"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.4",
        "ext-openssl": "*",
        "erag/laravel-lang-sync-inertia": "^1.6.0",
        "inertiajs/inertia-laravel": "^2.0.10",
        "laravel-notification-channels/webpush": "^10.2.0",
        "laravel/framework": "^12.38.1",
        "laravel/nightwatch": "^1.18.1",
        "laravel/octane": "^2.13.1",
        "laravel/socialite": "^5.23.1",
        "laravel/telescope": "^5.15.0",
        "laravel/tinker": "^2.10.1",
        "laravel/wayfinder": "^0.1.12",
        "php-ds/php-ds": "^1.7",
        "socialiteproviders/twitch": "^5.4.0",
        "spatie/laravel-data": "^4.18.0",
        "tightenco/ziggy": "^2.6.0"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.16.0",
        "barryvdh/laravel-ide-helper": "^3.6.0",
        "brianium/paratest": "^7.14.2",
        "fakerphp/faker": "^1.24.1",
        "laradumps/laradumps": "^4.5.3",
        "larastan/larastan": "^3.8.0",
        "laravel/boost": "^1.8.0",
        "laravel/pail": "^1.2.3",
        "laravel/pint": "^1.25.1",
        "laravel/sail": "^1.48.0",
        "mockery/mockery": "^1.6.12",
        "nunomaduro/collision": "^8.8.2",
        "pestphp/pest": "^4.1.3",
        "pestphp/pest-plugin-laravel": "^4.0.0",
        "phpunit/phpunit": "^12.3"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi",
            "@php -r \"file_exists('database/database.sqlite') || touch('database/database.sqlite');\"",
            "@php artisan migrate --graceful --ansi"
        ],
        "helper": [
            "@php artisan ide-helper:generate --helpers",
            "@php artisan ide-helper:meta",
            "@php artisan ide-helper:models --write-mixin"
        ],
        "lint": [
            "Composer\\Config::disableProcessTimeout",
            "./vendor/bin/phpstan analyse --memory-limit=2G"
        ],
        "dev": [
            "Composer\\Config::disableProcessTimeout",
            "bunx concurrently -c \"#93c5fd,#c4b5fd,#fb7185,#fdba74\" \"php artisan serve\" \"php artisan queue:listen --tries=1\" \"php artisan pail --timeout=0\" \"bun run dev\" --names=server,queue,logs,vite --kill-others"
        ],
        "dev:octane": [
            "Composer\\Config::disableProcessTimeout",
            "CADDY_SERVER_EXTRA_DIRECTIVES='@devtoolsjson path /.well-known/appspecific/com.chrome.devtools.json reverse_proxy @devtoolsjson localhost:5173' php artisan octane:start --watch --host=localhost"
        ],
        "debug:octane": [
            "Composer\\Config::disableProcessTimeout",
            "CADDY_SERVER_EXTRA_DIRECTIVES='@devtoolsjson path /.well-known/appspecific/com.chrome.devtools.json reverse_proxy @devtoolsjson localhost:5173' frankenphp php-cli -dxdebug.mode=debug -dxdebug.start_with_request=yes -dxdebug.client_host=127.0.0.1 -dxdebug.client_port=9003 artisan octane:start --watch --host=localhost"
        ],
        "dev:ssr": [
            "bun run build:ssr",
            "Composer\\Config::disableProcessTimeout",
            "bunx concurrently -c \"#93c5fd,#c4b5fd,#fb7185,#fdba74\" \"php artisan serve\" \"php artisan queue:listen --tries=1\" \"php artisan pail --timeout=0\" \"php artisan inertia:start-ssr\" --names=server,queue,logs,ssr --kill-others"
        ],
        "test": [
            "@php artisan config:clear --ansi",
            "@php artisan test"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": [
                "laravel/telescope"
            ]
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}